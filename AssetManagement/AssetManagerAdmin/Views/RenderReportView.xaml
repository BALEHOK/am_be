<UserControl x:Class="AssetManagerAdmin.View.RenderReportView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/printing"
      xmlns:dxrd="http://schemas.devexpress.com/winfx/2008/xaml/reportdesigner"
      xmlns:local="clr-namespace:AssetManagerAdmin.View"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      DataContext="{Binding RenderReport, Source={StaticResource Locator}}">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <dxrd:ReportDesigner x:Name="reportDesigner" />
        
    </Grid>
    
</UserControl>
